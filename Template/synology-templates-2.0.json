{
    "version": "2",
    "templates": [
        {
            "type": 1,
            "title": "Calibre-Web",
            "description": "Calibre-Web is a web app providing a clean interface for browsing, reading and downloading eBooks using a valid Calibre database.",
            "categories": [
                "Other",
                "eBooks"
            ],
            "platform": "linux",
            "logo": "https://cdn.jsdelivr.net/gh/walkxcode/dashboard-icons/png/calibreweb.png",
            "image": "lscr.io/linuxserver/calibre-web:latest",
            "name": "calibre-web",
            "env": [
                {
                    "default": "1000",
                    "label": "PUID",
                    "name": "PUID",
                    "description": "Docker host user ID. Obtain using 'id username'."
                },
                {
                    "default": "1000",
                    "label": "PGID",
                    "name": "PGID",
                    "description": "Docker host user group ID. Obtain using 'id username'."
                },
                {
                    "default": "Europe/London",
                    "label": "Time Zone",
                    "name": "TZ",
                    "description": "Set the time for the location of the container."
                },
                {
                    "default": "linuxserver/mods:universal-calibre",
                    "label": "DOCKER_MODS",
                    "name": "DOCKER_MODS",
                    "description": "(optional) x86-64 only. Adds the ability to perform ebook conversion."
                },
                {
                    "default": "1",
                    "label": "OAUTHLIB_RELAX_TOKEN_SCOPE",
                    "name": "OAUTHLIB_RELAX_TOKEN_SCOPE",
                    "description": "(optional) Set this to allow Google OAUTH to work."
                }
            ],
            "ports": [
                "8083:8083/tcp"
            ],
            "volumes": [
                {
                    "bind": "/volume2/docker/calibre-web/config",
                    "container": "/config"
                },
                {
                    "bind": "/volume1/Media",
                    "container": "/books"
                }
            ],
            "restart_policy": "unless-stopped"
        },
        {
            "type": 1,
            "title": "Code Server",
            "description": "Code-server is VS Code running on a remote server, accessible through the browser.",
            "categories": [
                "Other",
                "Tools"
            ],
            "platform": "linux",
            "logo": "https://raw.githubusercontent.com/SelfhostedPro/selfhosted_templates/master/Images/code-server.png",
            "image": "lscr.io/linuxserver/code-server:latest",
            "name": "code-server",
            "ports": [
                "8443:8443/tcp"
            ],
            "env": [
                {
                    "default": "1000",
                    "label": "PUID",
                    "name": "PUID"
                },
                {
                    "default": "1000",
                    "label": "PGID",
                    "name": "PGID"
                },
                {
                    "default": "Europe/London",
                    "label": "Time Zone",
                    "name": "TZ"
                },
                {
                    "label": "PASSWORD",
                    "name": "PASSWORD"
                },
                {
                    "label": "SUDO_PASSWORD",
                    "name": "SUDO_PASSWORD"
                },
                {
                    "default": "example.my.domain",
                    "label": "Proxied External URL",
                    "name": "PROXY_DOMAIN"
                },
                {
                    "default": "/workspace",
                    "label": "DEFAULT_WORKSPACE",
                    "name": "DEFAULT_WORKSPACE"
                }
            ],
            "volumes": [
                {
                    "bind": "/volume2/docker/code-server/config",
                    "container": "/config"
                },
                {
                    "bind": "/volume2/docker",
                    "container": "/workspace"
                }
            ],
            "restart_policy": "unless-stopped"
        },
        {
            "type": 1,
            "title": "Czkawka",
            "description": "Czkawka (tch•kav•ka (IPA: [ˈʧ̑kafka]), \"hiccup\" in Polish) is a simple, fast and free app to remove unnecessary files from your computer.",
            "categories": [
                "Tools",
                "Other"
            ],
            "platform": "linux",
            "logo": "https://raw.githubusercontent.com/qarmin/czkawka/018d5bebb0b297ba35529b03b8e2e68eb0a9b474/snap/gui/czkawka.png",
            "image": "jlesage/czkawka:latest",
            "name": "czkawka",
            "ports": [
                "5800:5800/tcp"
            ],
            "env": [
                {
                    "default": "1000",
                    "label": "User ID",
                    "name": "USER_ID",
                    "description": "Docker host user ID. Obtain using 'id username'."
                },
                {
                    "default": "1000",
                    "label": "Group ID",
                    "name": "GROUP_ID",
                    "description": "Docker host user group ID. Obtain using 'id username'."
                },
                {
                    "default": "en_GB.UTF-8",
                    "label": "Language",
                    "name": "LANG",
                    "description": "Set the locale, which defines the application's language, if supported. Format of the locale is language[_territory][.codeset], where language is an ISO 639 language code, territory is an ISO 3166 country code and codeset is a character set, like UTF-8. For example, Australian English using the UTF-8 encoding is en_AU.UTF-8."
                },
                {
                    "default": "Europe/London",
                    "label": "Time Zone",
                    "name": "TZ",
                    "description": "TimeZone used by the container. Timezone can also be set by mapping /etc/localtime between the host and the container."
                },
                {
                    "default": "1",
                    "label": "Dark Theme",
                    "name": "DARK_MODE",
                    "description": "When set to 1, dark mode is enabled for the application."
                }
            ],
            "volumes": [
                {
                    "bind": "/volume2/docker/czkawka/config",
                    "container": "/config"
                },
                {
                    "bind": "/volume1/<directory_to_scan>",
                    "container": "/storage"
                }
            ],
            "restart_policy": "unless-stopped"
        },
        {
            "type": 1,
            "title": "Dashy",
            "description": "Dashy is an open source, highly customizable, easy to use, privacy-respecting dashboard app.",
            "categories": [
                "Tools",
                "Web",
                "Other"
            ],
            "platform": "linux",
            "logo": "https://cdn.jsdelivr.net/gh/walkxcode/dashboard-icons/png/dashy.png",
            "image": "ghcr.io/lissy93/dashy:latest",
            "name": "dashy",
            "ports": [
                "4000:80/tcp"
            ],
            "env": [
                {
                    "default": "1000",
                    "label": "UID",
                    "name": "UID",
                    "description": "Docker host user ID. Obtain using 'id username'."
                },
                {
                    "default": "1000",
                    "label": "PGID",
                    "name": "GID",
                    "description": "Docker host user group ID. Obtain using 'id username'."
                },
                {
                    "default": "production",
                    "label": "Node Environment",
                    "name": "NODE_ENV",
                    "description": "Indicate whether Node should run in 'production' or 'development' environment."
                }
            ],
            "volumes": [
                {
                    "bind": "/volume2/docker/dashy/conf.yml",
                    "container": "/app/public/conf.yml"
                },
                {
                    "bind": "/volume2/docker/dashy/user-defined-themes.scss",
                    "container": "/app/src/styles/user-defined-themes.scss"
                },
                {
                    "bind": "/volume2/docker/dashy/fonts",
                    "container": "/app/public/fonts"
                },
                {
                    "bind": "/volume2/docker/dashy/item-icons",
                    "container": "/app/public/item-icons"
                }
            ],
            "restart_policy": "unless-stopped"
        },
        {
            "type": 1,
            "title": "ddclient",
            "description": "DDClient is a Perl based IP update client for *nix based systems including Linux, FreeBSD and Mac OS X. It supports operating as a daemon as well as optimizes updates for multiple hostnames and aliases. https://ddclient.net",
            "categories": [
                "Internet",
                "Other"
            ],
            "platform": "linux",
            "logo": "https://raw.githubusercontent.com/ghost-of-cerberus/portainer_templates/main/Template/Images/ddclient-logo-trans.png",
            "image": "lscr.io/linuxserver/ddclient:latest",
            "name": "ddclient",
            "env": [
                {
                    "default": "1000",
                    "label": "PUID",
                    "name": "PUID",
                    "description": "Docker host user ID. Obtain using 'id username'."
                },
                {
                    "default": "1000",
                    "label": "PGID",
                    "name": "PGID",
                    "description": "Docker host user group ID. Obtain using 'id username'."
                },
                {
                    "default": "Europe/London",
                    "label": "Time Zone",
                    "name": "TZ",
                    "description": "Set the time for the location of the container."
                }
            ],
            "volumes": [
                {
                    "bind": "/volume2/docker/ddclient",
                    "container": "/config"
                }
            ],
            "restart_policy": "always"
        },
        {
            "type": 1,
            "title": "Emby",
            "description": "Emby organizes video, music, live TV, and photos from personal media libraries and streams them to smart TVs, streaming boxes and mobile devices.",
            "categories": [
                "Video",
                "Other"
            ],
            "platform": "linux",
            "logo": "https://cdn.jsdelivr.net/gh/walkxcode/dashboard-icons/png/emby.png",
            "image": "lscr.io/linuxserver/emby:latest",
            "name": "emby",
            "env": [
                {
                    "default": "1000",
                    "label": "PUID",
                    "name": "PUID",
                    "description": "Docker host user ID. Obtain using 'id username'."
                },
                {
                    "default": "1000",
                    "label": "PGID",
                    "name": "PGID",
                    "description": "Docker host user group ID. Obtain using 'id username'."
                },
                {
                    "default": "Europe/London",
                    "label": "Time Zone",
                    "name": "TZ",
                    "description": "Set the time for the location of the container."
                }
            ],
            "ports": [
                "8096:8096/tcp",
                "8920:8920/tcp"
            ],
            "volumes": [
                {
                    "bind": "/volume2/docker/emby/config",
                    "container": "/config"
                },
                {
                    "bind": "/volume1/Video",
                    "container": "/data/video"
                }
            ],
            "restart_policy": "unless-stopped"
        },
        {
            "type": 1,
            "title": "Flame (fdarveau)",
            "description": "[Fork of Flame with App Categories] Flame is self-hosted startpage for your server. Its design is inspired (heavily) by SUI. Flame is very easy to setup and use. With built-in editors, it allows you to setup your very own application hub in no time - no file editing necessary.",
            "categories": [
                "Tools",
                "Web",
                "Other"
            ],
            "platform": "linux",
            "logo": "https://cdn.jsdelivr.net/gh/walkxcode/dashboard-icons/png/flame.png",
            "image": "ghcr.io/fdarveau/flame:latest",
            "name": "flame",
            "ports": [
                "5005:5005/tcp"
            ],
            "env": [
                {
                    "default": "P@ssw0rd",
                    "label": "Password",
                    "name": "PASSWORD",
                    "description": "Enter Flame Login Password"
                }
            ],
            "volumes": [
                {
                    "bind": "/volume2/docker/flame",
                    "container": "/app/data"
                }
            ],
            "restart_policy": "unless-stopped"
        },
        {
            "type": 1,
            "title": "Grav",
            "description": "Grav is not your average CMS. It's an open source, flat-file NoDB CMS written for PHP 7.1.3+ that is focused on being crazy fast, easy to learn and use, with powerful features to enable it be easily extended.",
            "categories": [
                "Web",
                "Other"
            ],
            "platform": "linux",
            "logo": "https://cdn.jsdelivr.net/gh/walkxcode/dashboard-icons/png/grav.png",
            "image": "lscr.io/linuxserver/grav:latest",
            "name": "",
            "note": "Volume directory must be changed to separate name for each site.",
            "ports": [
                "80:80/tcp"
            ],
            "env": [
                {
                    "default": "1000",
                    "label": "PUID",
                    "name": "PUID",
                    "description": "Docker host user ID. Obtain using 'id username'."
                },
                {
                    "default": "1000",
                    "label": "PGID",
                    "name": "PGID",
                    "description": "Docker host user group ID. Obtain using 'id username'."
                },
                {
                    "default": "Europe/London",
                    "label": "Time Zone",
                    "name": "TZ",
                    "description": "Set the time for the location of the container."
                }
            ],
            "volumes": [
                {
                    "bind": "/volume2/docker/grav/changeme",
                    "container": "/config"
                }
            ],
            "restart_policy": "unless-stopped"
        },
        {
            "type": 1,
            "title": "Magma",
            "description": "Personal dashboard that is highly customizable, lightweight and responsive.",
            "categories": [
                "Tools",
                "Web",
                "Other"
            ],
            "platform": "linux",
            "logo": "https://i.imgur.com/K4sSC3X.png",
            "image": "help14/magma:latest",
            "name": "magma",
            "ports": [
                "7001:7001/tcp"
            ],
            "volumes": [
                {
                    "bind": "/volume2/docker/magma",
                    "container": "/app/data"
                }
            ],
            "restart_policy": "unless-stopped"
        },
        {
            "type": 1,
            "title": "Nginx Proxy Manager",
            "description": "(Alpine) Nginx Proxy Manager enables you to easily forward to your websites running at home or otherwise, including free SSL, without having to know too much about Nginx or Letsencrypt.",
            "categories": [
                "Proxy",
                "Tools"
            ],
            "platform": "linux",
            "logo": "https://cdn.jsdelivr.net/gh/walkxcode/dashboard-icons/png/nginxproxymanager.png",
            "image": "gh0stofcerberos/nginx-proxy-manager:latest",
            "name": "nginx-proxy-manager",
            "ports": [
                "80:80/tcp",
                "8181:8181/tcp",
                "443:443/tcp"
            ],
            "volumes": [
                {
                    "bind": "/volume2/docker/nginx-proxy-manager/config",
                    "container": "/config"
                },
                {
                    "bind": "/volume2/docker/nginx-proxy-manager/nginx/custom",
                    "container": "/config/nginx/custom"
                },
                                {
                    "bind": "/volume2/docker/nginx-proxy-manager/nginx/snippets",
                    "container": "/config/nginx/snippets"
                },
                                {
                    "bind": "/volume2/docker/nginx-proxy-manager/nginx/geoip",
                    "container": "/config/nginx/geoip"
                }
            ],
            "restart_policy": "unless-stopped"
        },
        {
            "type": 1,
            "title": "Navidrome",
            "description": "Software that allows you to listen to your own digital music in the same way you would with services like Spotify, Apple Music and others.",
            "categories": [
                "Music",
                "Other"
            ],
            "platform": "linux",
            "logo": "https://cdn.jsdelivr.net/gh/walkxcode/dashboard-icons/png/navidrome.png",
            "image": "deluan/navidrome:latest",
            "name": "navidrome",
            "note": "Check Navidrome site for latest env variable options",
            "ports": [
                "4533:4533/tcp"
            ],
            "env": [
                {
                    "default": "/data/navidrome.toml",
                    "label": "Config File Path",
                    "name": "ND_CONFIGFILE",
                    "description": "Load configurations from an external config file."
                }
            ],
            "volumes": [
                {
                    "bind": "/volume2/docker/navidrome/data",
                    "container": "/data"
                },
                {
                    "bind": "/volume1/Audio",
                    "container": "/music",
                    "readonly": true
                }
            ],
            "restart_policy": "unless-stopped"
        }
    ]
}
